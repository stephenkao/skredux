{
	"parser": "babel-eslint",
	"plugins": [
		"react"
	],
	"env": {
		"browser": true,
		"node": true
	},
	"ecmaFeatures": {
		"arrowFunctions": true,
		"blockBindings": true,
		"experimentalObjectRestSpread": true,
		"generators": true,
		"jsx": true,
		"module": true,
		"modules": true,
		"restParams": true,
		"spread": true
	},
	"globals": {
		"describe": true,
		"it": true,
		"before": true,
		"after": true,
		"beforeEach": true,
		"afterEach": true,
		"expect": true,
		"should": true,
		"$": true,
		"jQuery": true,
		"Modernizr": true,
		"_fbq": true,
		"FB": true,
		"ga": true,
		"templateVars": true,
		"_usq": true,
		"optimizely": true
	},
	"rules": {
		"quotes": [
			2,
			"single",
			"avoid-escape"
		],
		"no-sequences": 2,
		"no-var": 2,
		"one-var": [
			2,
			{
				"uninitialized": "always",
				"initialized": "never"
			}
		],
		"space-before-function-paren": [
			2,
			"never"
		],
		"no-irregular-whitespace": 2,
		"no-dupe-keys": 2,
		"block-scoped-var": 2,
		"no-implied-eval": 2,
		"yoda": 2,
		"consistent-return": 2,
		"no-multi-str": 2,
		"no-cond-assign": [
			2,
			"always"
		],
		"no-extra-bind": 2,
		"space-before-blocks": 2,
		"no-extra-semi": 2,
		"no-self-compare": 2,
		"no-floating-decimal": 2,
		"no-eq-null": 2,
		"no-ex-assign": 2,
		"guard-for-in": 2,
		"eqeqeq": 2,
		"no-native-reassign": 2,
		"no-new-object": 0,
		"no-inner-declarations": 2,
		"no-loop-func": 2,
		"no-sparse-arrays": 2,
		"no-with": 2,
		"no-extend-native": 2,
		"no-unused-vars": [
			1,
			{
				"vars": "local",
				"args": "none"
			}
		],
		"jsx-quotes": 2,
		"semi": [
			1,
			"always"
		],
		"no-caller": 2,
		"strict": [
			2,
			"never"
		],
		"use-isnan": 2,
		"camelcase": [
			2,
			{
				"properties": "never"
			}
		],
		"wrap-iife": [
			2,
			"any"
		],
		"no-empty": 2,
		"indent": [
			1,
			2,
			{
				"SwitchCase": 1
			}
		],
		"space-return-throw-case": 2,
		"no-duplicate-case": 2,
		"no-unused-expressions": [
			1,
			{
				"allowShortCircuit": true,
				"allowTernary": true
			}
		],
		"comma-spacing": [
			2,
			{
				"before": false,
				"after": true
			}
		],
		"eol-last": 2,
		"no-shadow-restricted-names": 2,
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"default-case": 0,
		"no-eval": 2,
		"no-redeclare": 2,
		"no-shadow": 0,
		"comma-style": [
			2,
			"last"
		],
		"no-invalid-regexp": 2,
		"no-lone-blocks": 2,
		"no-constant-condition": 1,
		"no-undef": 2,
		"radix": 2,
		"no-use-before-define": 2,
		"comma-dangle": [
			1,
			"never"
		],
		"curly": [
			2,
			"multi-line"
		],
		"no-extra-boolean-cast": 0,
		"no-new-func": 2,
		"no-throw-literal": 2,
		"no-octal": 2,
		"no-fallthrough": 2,
		"no-else-return": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"no-new-wrappers": 2,
		"no-unreachable": 2,
		"no-param-reassign": 0,
		"dot-notation": [
			2,
			{
				"allowKeywords": true
			}
		],
		"no-obj-calls": 2,
		"no-underscore-dangle": 0,
		"no-octal-escape": 2,
		"padded-blocks": [
			0,
			"never"
		],
		"no-extra-parens": [
			2,
			"functions"
		],
		"key-spacing": [
			2,
			{
				"beforeColon": false,
				"afterColon": true
			}
		],
		"semi-spacing": [
			2,
			{
				"before": false,
				"after": true
			}
		],
		"no-proto": 2,
		"no-func-assign": 2,
		"space-after-keywords": 2,
		"space-infix-ops": 2,
		"no-script-url": 2,
		"no-return-assign": 2,
		"vars-on-top": 2,
		"no-multiple-empty-lines": [
			2,
			{
				"max": 2
			}
		],
		"no-nested-ternary": 2,
		"no-debugger": 0,
		"no-new": 0,
		"new-cap": [
			2,
			{
				"newIsCap": true,
				"capIsNew": false,
				"capIsNewExceptions": [
					"RIT",
					"Store",
					"Actions"
				]
			}
		],
		"prefer-const": 2,
		"react/display-name": 0,
		"react/jsx-boolean-value": 2,
		"react/jsx-no-undef": 2,
		"react/jsx-sort-props": 0,
		"react/jsx-sort-prop-types": 0,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2,
		"react/no-did-mount-set-state": [
			2,
			"allow-in-func"
		],
		"react/no-did-update-set-state": 2,
		"react/no-multi-comp": 2,
		"react/no-unknown-property": 2,
		"react/prop-types": 2,
		"react/react-in-jsx-scope": 2,
		"react/self-closing-comp": 2,
		"react/wrap-multilines": 2,
		"react/sort-comp": [
			2,
			{
				"order": [
					"lifecycle",
					"/^on.+$/",
					"/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
					"everything-else",
					"/^render.+$/",
					"render"
				]
			}
		]
	}
}